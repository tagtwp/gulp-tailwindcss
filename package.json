{
	"name": "gulp-tailwindcss",
	"version": "1.0.0",
	"description": "Gulp boilerplate for tailwindcss",
	"main": "index.js",
	"scripts": {
		"dev": "gulp",
		"build": "gulp build",
		"lint:prettier": "npx prettier --check .",
		"lint": "run-p \"lint:*\"",
		"lint-fix:prettier": "npx prettier --write .",
		"lint-fix": "run-p \"lint-fix:*\"",
		"code": "editorconfig-checker",
		"clean": "gulp cleanDist && gulp cleanTemp",
		"deploy": "gulp && gh-pages -d dist"
	},
	"keywords": [
		"gulp",
		"tailwindcss"
	],
	"author": "tagtwp",
	"license": "ISC",
