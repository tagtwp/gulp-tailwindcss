		"babel-loader": "^9.1.3",
		"browser-sync": "^3.0.2",
		"cssnano": "^6.0.1",
		"del": "^6.1.1",
		"editorconfig-checker": "^5.1.8",
		"gh-pages": "^6.1.1",
		"gulp": "^5.0.0",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-cached": "^1.1.1",
		"gulp-file-include": "^2.3.0",
		"gulp-htmlmin": "^5.0.1",
		"gulp-if": "^3.0.0",
